{
    "name": "@kodus/kodus-common",
    "author": "Kodus",
    "description": "Common types and utilities used across Kodus projects",
    "type": "commonjs",
    "repository": {
        "type": "git",
        "url": "https://github.com/kodustech/kodus-common"
    },
    "version": "1.3.6",
    "publishConfig": {
        "access": "public",
        "registry": "https://us-central1-npm.pkg.dev/$GAR_PROJECT_ID/kodus-pkg/"
    },
    "license": "AGPL-3.0-only",
    "scripts": {
        "prepack": "rm -rf dist && yarn generate-barrels && yarn types",
        "prepack:watch": "yarn generate-barrels && tsc -b --watch tsconfig.build.json",
        "generate-barrels": "barrelsby --delete -c barrelsby.json",
        "types": "tsc -b --force tsconfig.build.json && tsc-alias tsconfig.build.json",
        "test": "jest --runInBand --detectOpenHandles --forceExit",
        "publish:gar": "source scripts/refresh-token.sh && npm publish --registry=https://us-central1-npm.pkg.dev/$GAR_PROJECT_ID/kodus-pkg/ --access public",
        "refresh-token": "source scripts/refresh-token.sh",
        "publish:quick": "source scripts/refresh-token.sh && yarn prepack && yarn publish:gar"
    },
    "files": [
        "dist/**/*"
    ],
    "exports": {
        "./types": "./dist/types/index.js",
        "./llm": "./dist/llm/index.js",
        "./utils": "./dist/utils/index.js",
        "./interfaces": "./dist/interfaces/index.js",
        "./package.json": "./package.json"
    },
    "typesVersions": {
        "*": {
            "types": [
                "./dist/types"
            ],
            "llm": [
                "./dist/llm"
            ],
            "utils": [
                "./dist/utils"
            ],
            "interfaces": [
                "./dist/interfaces"
            ]
        }
    },
    "dependencies": {},
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.3",
        "@eslint/js": "^9.39.2",
        "@langchain/anthropic": "^1.3.10",
        "@langchain/community": "^1.1.4",
        "@langchain/core": "^1.1.15",
        "@langchain/google-gauth": "^2.1.10",
        "@langchain/google-vertexai": "^2.1.10",
        "@langchain/openai": "^1.2.2",
        "@nestjs/common": "^11.1.12",
        "@nestjs/core": "^11.1.12",
        "barrelsby": "^2.8.1",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.5",
        "jest": "^30.2.0",
        "prettier": "^3.8.0",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.2",
        "ts-loader": "^9.5.4",
        "tsc-alias": "^1.8.16",
        "tslib": "^2.8.1",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.53.0",
        "zod": "^4.2.1",
        "zod-to-json-schema": "^3.24.6"
    },
    "peerDependencies": {
        "@langchain/anthropic": "^1.3.10",
        "@langchain/community": "^1.1.4",
        "@langchain/core": "^1.1.15",
        "@langchain/google-gauth": "^2.1.10",
        "@langchain/google-vertexai": "^2.1.10",
        "@langchain/openai": "^1.2.2",
        "@nestjs/common": "^11.1.12",
        "@nestjs/core": "^11.1.12",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.2",
        "tslib": "^2.8.1",
        "zod": "^4.2.1",
        "zod-to-json-schema": "^3.25.0"
    },
    "jest": {
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/src/$1",
            "^test/mocks/(.*)$": "<rootDir>/test/mocks/$1"
        },
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "moduleDirectories": [
            "node_modules",
            "src",
            "test"
        ],
        "rootDir": ".",
        "roots": [
            "src",
            "test"
        ],
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
